# Variables de entorno para FinSmart Docker

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
BOT_USERNAME=@your_bot_username

# Base de datos PostgreSQL
POSTGRES_DB=finsmart
POSTGRES_USER=finsmart_user
POSTGRES_PASSWORD=finsmart_password
DATABASE_URL=postgresql://finsmart_user:finsmart_password@postgres:5432/finsmart

# Backend
JWT_SECRET=your-secret-key-change-in-production
API_PORT=8000

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000

# IA Service
OLLAMA_HOST=http://ollama:11434
IA_SERVICE_PORT=8001
